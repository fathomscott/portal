{% extends '@Admin/layout.html.twig' %}

{% form_theme form '@Admin/form_theme.html.twig' %}

{% block title %}{{ 'navigation.manage'|trans }} {{ 'navigation.system_default.system_defaults'|trans }}{% endblock %}

{% block breadcrumb %}
    <li class="active">{{ 'navigation.system_default.system_defaults'|trans }}</li>
{% endblock %}

{% block content %}
    {{ form_start(form) }}
    {% for field in form %}
        {{ form_row(field) }}
    {% endfor %}

    {# buttons #}
    <div class="buttons col-md-offset-3 m-t-10">
        <button type="submit" class="btn btn-warning m-r-10">{{ 'verb.submit'|trans }}</button>
    </div>
    {{ form_end(form) }}

{% endblock content %}
